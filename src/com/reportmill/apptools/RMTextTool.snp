<?xml version="1.0" encoding="UTF-8"?>
<TabView>
  <SpringView x="11" y="30" width="254" height="282" PrefWidth="254" PrefHeight="282" GrowWidth="true" GrowHeight="true" title="Text">
    <RowView width="254" height="34" Padding="4,8,3,8" GrowWidth="true">
      <ToggleButton Name="AlignLeftButton" PrefWidth="26" PrefHeight="24" image="AlignLeft.png" Position="CENTER_LEFT" ToggleGroup="bg1" />
      <ToggleButton Name="AlignCenterButton" PrefWidth="26" PrefHeight="24" image="AlignCenter.png" Position="CENTER" ToggleGroup="bg1" />
      <ToggleButton Name="AlignRightButton" PrefWidth="26" PrefHeight="24" image="AlignRight.png" Position="CENTER" ToggleGroup="bg1" />
      <ToggleButton Name="AlignFullButton" PrefWidth="26" PrefHeight="24" image="AlignFull.png" Position="CENTER_RIGHT" ToggleGroup="bg1" />
      <ToggleButton Name="AlignTopButton" PrefWidth="26" PrefHeight="24" LeanX="RIGHT" image="AlignTop.png" Position="CENTER_LEFT" ToggleGroup="bg2" />
      <ToggleButton Name="AlignMiddleButton" PrefWidth="26" PrefHeight="24" image="AlignMiddle.png" Position="CENTER" ToggleGroup="bg2" />
      <ToggleButton Name="AlignBottomButton" PrefWidth="26" PrefHeight="24" image="AlignBottom.png" Position="CENTER_RIGHT" ToggleGroup="bg2" />
    </RowView>
    <TextView Name="TextView" x="2" y="35" width="250" height="120" GrowWidth="true" GrowHeight="true" />
    <Label x="6" y="152" width="108" height="20" LeanY="BOTTOM" text="Margin (Tp,Lt,Bm,Rt):" />
    <TextField Name="MarginText" x="2" y="172" width="117" height="22" LeanY="BOTTOM" />
    <Label x="133" y="152" width="99" height="20" LeanX="RIGHT" LeanY="BOTTOM" text="Rounding Radius:" />
    <ThumbWheel Name="RoundingThumb" x="129" y="175" width="51" height="16" LeanX="RIGHT" LeanY="BOTTOM" max="20" absmin="0" round="1" />
    <TextField Name="RoundingText" x="183" y="172" width="49" height="22" LeanX="RIGHT" LeanY="BOTTOM" />
    <Label x="2" y="199" width="118" height="20" LeanY="BOTTOM" text="Overflow Behavior:" />
    <RadioButton Name="GrowRadio" x="4" y="219" width="130" height="20" LeanY="BOTTOM" text="Grow (if in row)" ToggleGroup="bg3" />
    <RadioButton Name="ShrinkRadio" x="4" y="239" width="130" height="20" LeanY="BOTTOM" text="Shrink Text to Fit" ToggleGroup="bg3" />
    <RadioButton Name="PaginateRadio" x="4" y="259" width="130" height="20" LeanY="BOTTOM" text="Paginate (if in page)" ToggleGroup="bg3" />
    <Label x="121" y="199" width="120" height="20" LeanX="RIGHT" LeanY="BOTTOM" text="Misc. Options:" />
    <CheckBox Name="ShowBorderCheckBox" x="123" y="219" width="140" height="20" LeanX="RIGHT" LeanY="BOTTOM" text="Always Show Border" />
    <CheckBox Name="CoalesceNewlinesCheckBox" x="123" y="239" width="140" height="20" LeanX="RIGHT" LeanY="BOTTOM" text="Coalesce Newlines" />
  </SpringView>
  <SpringView x="11" y="30" width="254" height="282" PrefWidth="254" PrefHeight="282" GrowWidth="true" GrowHeight="true" title="Advanced">
    <ThumbWheel Name="CharSpacingThumb" x="4" y="23" width="120" height="16" type="linear" min="-10" max="10" round="0.1" />
    <TextField Name="CharSpacingText" x="128" y="21" width="50" height="20" Align="CENTER" />
    <Label x="4" y="4" width="119" height="14" text="Char Spacing" />
    <ThumbWheel Name="LineSpacingThumb" x="4" y="71" width="120" height="16" type="linear" max="3" absmin="0" round="0.05" />
    <TextField Name="LineSpacingText" x="128" y="69" width="50" height="20" Align="CENTER" />
    <Label x="4" y="52" width="80" height="14" text="Line Spacing" />
    <Button Name="LineSpacingSingleButton" x="87" y="52" width="46" height="15" text="Single">
      <font name="Arial" size="9" />
    </Button>
    <Button Name="LineSpacingDoubleButton" x="134" y="52" width="46" height="15" text="Double">
      <font name="Arial" size="9" />
    </Button>
    <Label x="4" y="99" width="80" height="14" text="Line Gap" />
    <ThumbWheel Name="LineGapThumb" x="4" y="118" width="120" height="16" type="linear" max="20" round="0.1" />
    <TextField Name="LineGapText" x="128" y="116" width="50" height="20" Align="CENTER" />
    <Label x="4" y="163" width="50" height="14" text="Line Min:" />
    <Label x="128" y="163" width="49" height="14" text="Line Max:" />
    <Spinner Name="LineHeightMinSpinner" x="54" y="159" width="60" height="24" Value="0" />
    <Spinner Name="LineHeightMaxSpinner" x="179" y="159" width="60" height="24" Value="0" />
    <MenuButton x="4" y="195" width="100" height="22" text="Text Actions">
      <MenuItem Name="MakeMinWidthMenuItem" text="Make Min Width" />
      <MenuItem Name="MakeMinHeightMenuItem" text="Make Min Height" />
      <MenuItem Name="TurnToPathMenuItem" text="Turn to Path" />
      <MenuItem Name="TurnToCharsShapeMenuItem" text="Turn to Chars Shape" />
      <MenuItem Name="LinkedTextMenuItem" text="Create Linked Text" />
    </MenuButton>
    <CheckBox Name="PerformWrapCheckBox" x="4" y="250" width="250" height="20" LeanX="RIGHT" LeanY="BOTTOM" text="Wrap text around peers" />
  </SpringView>
  <ColView Padding="20,10,10,10" Spacing="5" title="PDF">
    <CheckBox Name="EditableCheckBox" text="Editable" />
    <CheckBox Name="MultilineCheckBox" text="Multiline" />
  </ColView>
</TabView>